{
    "server": {
        "address": "$CLAVE_ADDRESS",
        "port": $CLAVE_PORT,
        "cert_dir_path": "$CLAVE_CERT_DIR",
        "log_level": "WARN",
        "secret": "$CLAVE_SECRET",
        "inactivity_timeout": 60
    },
    "database": {
        "user": "$DB_USER",
        "password": "$DB_PASS",
        "port": $DB_PORT,
        "address": "$DB_ADDRESS",
        "timeout": 1,
        "https_enabled": false
    },
    "modules": {
        "user": "clave-3rd-party",
        "path": "$MODULE_DIR",
        "max_wait_runtime": 5
    }
}